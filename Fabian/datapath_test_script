add wave *
force clk 0,1 5ns -r 10ns
force reset '1'
force dataIn "00000000000011010000011111000000"
force rd "0001"
force AMux '0'
force rs "1111"
force CMux '1'
force rr '1'
force io "01"
force ALU "100"
run 20ns 
force rs "0010"
run 50ns
force AMux '1'
force CMux '0'
run 75ns
force rd "0010"
run 75ns
force dataIn "00001110000011010000011111000000"
run 75ns
force ALU "101"
run 150ns
force rd "0010"
